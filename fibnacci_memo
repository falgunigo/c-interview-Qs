using System;
					
public class Program
{
	public static void Main()
	{
		Console.WriteLine("Fibonnaci Numbers");
		int n = Int32.Parse(Console.ReadLine());
		Program p = new Program();
		p.allFib(n);
	}
	
	void allFib(int n){
	for(int i = 0; i<=n; i++){
			Console.Write(fib(i)+" ");
		}
	}
	
	int fib(int i){
	if (i<=0){
	return 0;
	}
	else if (i==1) return 1;
	else return fib(i-1) + fib(i-2);	
	}
}
